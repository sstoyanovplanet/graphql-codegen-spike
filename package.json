{
	"name": "graphql-codegen-spike",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"codegen:dev": "env-cmd -e dev graphql-codegen --config ./src/utils/graphql/codegen.ts",
		"codegen:test": "env-cmd -e test graphql-codegen --config ./src/utils/graphql/codegen.ts"
	},
	"dependencies": {
		"@apollo/client": "^3.11.8",
		"env-cmd": "^10.1.0",
		"graphql": "^16.9.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"@graphql-codegen/cli": "^5.0.3",
		"@graphql-codegen/near-operation-file-preset": "^3.0.0",
		"@graphql-codegen/typescript": "^4.1.0",
		"@graphql-codegen/typescript-react-apollo": "^4.3.2",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.2",
		"eslint": "^9.11.1",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.12",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"prettier-eslint": "^16.3.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.7.0",
		"vite": "^5.4.8"
	}
}
